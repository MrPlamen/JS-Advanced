function solve(numOne, numTwo, mathOperation) {
    switch (mathOperation) {
        case '+':
            console.log(numOne + numTwo);
        break;
        case '-':
            console.log(numOne - numTwo);
        break;    
        case '*':
            console.log(numOne * numTwo);
        break;    
        case '/':
            console.log(numOne / numTwo);
        break;  
        case '%':
            console.log(numOne % numTwo);
        break; 
        case '**':
            console.log(numOne ** numTwo);
        break; 
        default:
        break;
    }
}
